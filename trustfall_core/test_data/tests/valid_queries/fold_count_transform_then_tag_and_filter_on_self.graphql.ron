TestGraphQLQuery (
    schema_name: "numbers",
    query: r#"
{
    Number(min: 28, max: 31) {
        ... on Composite {
            primeFactor
                @fold
                @transform(op: "count")
                @tag(name: "tagged")
                @transform(op: "abs")
                @filter(op: "=", value: ["%tagged"])
                @output
            {
                factors: value @output
            }
        }
    }
}"#,
    arguments: {},
)
