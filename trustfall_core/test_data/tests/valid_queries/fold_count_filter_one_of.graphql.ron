TestGraphQLQuery (
    // We use a negative number as a filter to ensure that we properly handle the negative number case in get_max_fold_count_limit.
    schema_name: "numbers",
    query: r#"
{
    Number(min: 4, max: 6) {
        ... on Composite {
            value @output

            primeFactor @fold @transform(op: "count") @filter(op: "one_of", value: ["$neg_two"]) {
                factors: value @output
            }
        }
    }
}"#,
    arguments: {
        "neg_two": List([Int64(-2)]),
    },
)
