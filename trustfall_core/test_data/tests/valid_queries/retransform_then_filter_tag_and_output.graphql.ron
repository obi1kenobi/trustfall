TestGraphQLQuery (
    schema_name: "numbers",
    query: r#"
{
    Number(min: -1, max: 3) {
        value @tag

        vowelsInName
            @output
            @transform(op: "len")
            @output
            @transform(op: "+", value: ["%value"])
            @filter(op: "<=", value: ["$four"])
            @output
    }
}"#,
    arguments: {
        "four": Int64(4),
    },
)
