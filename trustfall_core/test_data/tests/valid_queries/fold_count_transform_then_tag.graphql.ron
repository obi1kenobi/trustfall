TestGraphQLQuery (
    schema_name: "numbers",
    query: r#"
{
    Four {
        primeFactor
            @fold
            @transform(op: "count")
            @transform(op: "abs")
            @tag(name: "primes_count")

        predecessor {
            value @filter(op: ">", value: ["%primes_count"]) @output
        }
    }
}"#,
    arguments: {},
)
