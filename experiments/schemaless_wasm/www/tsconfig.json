{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "target": "esnext",
        "allowSyntheticDefaultImports": true,
        "lib": ["DOM", "ESNext", "ESNext.Disposable"]
    }
}
