# Items where lints were allowed. Not all crates have these,
# try one of: anyhow, httparse, hyper, itertools.
query {
  Crate {
    item {
      name @output

      attribute {
        raw_attribute @output

        content {
          base @filter(op: "=", value: ["$allow_lint"])
        }
      }

      span {
        filename @output
        begin_line @output
      }
    }
  }
}

vars:
{
  "allow_lint": "allow"
}
